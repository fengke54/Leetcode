class Solution(object):
    def trailingZeroes(self, n):
        """
        :type n: int
        :rtype: int
        """
        
        
        # calculate factorial 
        n_factorial = 1
        for i in range(2, n+1):
            n_factorial *= i
            
        # count trailing zeros
        count = 0 
        while n_factorial % 10 == 0:
            count += 1
            n_factorial //= 10 
